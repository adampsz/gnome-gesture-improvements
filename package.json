{
  "name": "gnome-gestures",
  "version": "1.0.0",
  "description": "Touchpad Gesture Improvements",
  "main": "extension.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:extension": "eslint extension scripts tests",
    "lint:package": "eslint build --fix",
    "transpile:watch": "tsc --build --watch",
    "transpile": "npm run lint:extension && tsc --build",
    "build": "npm run clean && npm run transpile && npm run lint:package",
    "clean": "rm -rf build && mkdir build",
    "pack": "npm run build && make pack",
    "update": "npm run pack && make update"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.gnome.org/harshadgavali/gnome-gestures.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@girs/gio-2.0": "^2.77.0-3.2.8",
    "@girs/gjs": "^3.2.8",
    "@girs/glib-2.0": "^2.77.0-3.2.8",
    "@girs/gnome-shell": "^45.0.0-beta9",
    "@girs/gobject-2.0": "^2.77.0-3.2.8",
    "@girs/gtk-4.0": "^4.12.0-3.2.8",
    "@types/glob": "^7.2.0",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "glob": "^7.2.0",
    "path": "^0.12.7",
    "typescript": "^4.6.3",
    "yargs": "^17.4.0"
  }
}
